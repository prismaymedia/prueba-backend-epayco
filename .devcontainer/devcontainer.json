{
  "name": "Node.js Client API Container",
  "workspaceFolder": "/code",
  "mounts": [
    {
      "source": "${localWorkspaceFolder}",
      "target": "/code",
      "type": "bind"
    }
  ],
  "customizations": {
    "vscode": {
      "extensions": [
        "dbaeumer.vscode-eslint",
        "esbenp.prettier-vscode",
        "Gruntfuggly.todo-tree",
        "Prisma.prisma"
      ],
      "settings": {
      }
    }
  },
  "postCreateCommand": "if [ -f package-lock.json ]; then npm install; fi",
  "dockerComposeFile": "../docker-compose.yml",
  "service": "api-epayco",
  "runServices": [],
  "overrideCommand": true
}